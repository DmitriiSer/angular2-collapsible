{
  "name": "angular2-collapsible",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && npm run zip",
    "build": "ng build && cpx README.md dist",
    "test": "ng test",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "zip": "cd ./dist && npm pack",
    "updateVersion": "cd ./projects/angular2-collapsible && npm version patch"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/animations": "^6.1.0",
    "@angular/cli": "~6.2.6",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@types/jasmine": "^2.8.9",
    "codelyzer": "^4.5.0",
    "cpx": "^1.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.1.0",
    "rxjs": "^6.2.2",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2",
    "zone.js": "^0.8.26"
  }
}
